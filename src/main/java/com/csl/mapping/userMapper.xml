<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.csl.dao.userMapperDao" >
    <resultMap id="BaseResultMap" type="com.csl.entity.User" >
        <id column="id" property="id" jdbcType="INTEGER" />
        <result column="name" property="name" jdbcType="VARCHAR" />
        <result column="password" property="password" jdbcType="VARCHAR" />
    </resultMap>
    <select id="select" resultType="com.csl.entity.User" parameterType="java.lang.Integer">
        SELECT * FROM user
    </select>
    <select id="info" resultType="java.lang.String" parameterType="java.lang.String">
        SELECT name FROM user WHERE name=#{name}
    </select>
</mapper>